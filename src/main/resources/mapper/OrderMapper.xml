<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.ecommerce.mapper.OrderMapper">
    <insert id="insertOrder" useGeneratedKeys="true" keyProperty="id" parameterType="com.example.ecommerce.model.Order">
        insert into `order`(user_id,customer_name,customer_email,total_price,created_at)
        values(#{userId},#{customerName},#{customerEmail},#{totalPrice},#{createdAt})
    </insert>
</mapper>